{
    "version": 2,
    "name": "ng-ecommerce",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}